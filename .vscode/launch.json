    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) game_of_life",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/build/game_of_life",
            "args": "",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "preLaunchTask": "build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}